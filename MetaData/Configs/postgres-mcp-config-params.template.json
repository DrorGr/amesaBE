{
  "mcpServers": {
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres"
      ],
      "env": {
        "POSTGRES_HOST": "HOST",
        "POSTGRES_PORT": "5432",
        "POSTGRES_DATABASE": "DATABASE",
        "POSTGRES_USER": "USERNAME",
        "POSTGRES_PASSWORD": "PASSWORD",
        "POSTGRES_SSL_MODE": "require"
      }
    }
  }
}
