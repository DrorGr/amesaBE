{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "JwtSettings": {
    "SecretKey": "",
    "Issuer": "AmesaAuthService",
    "Audience": "AmesaFrontend",
    "ExpiryInMinutes": "60",
    "RefreshTokenExpiryInDays": "7"
  },
  "Authentication": {
    "Google": {
      "ClientId": "",
      "ClientSecret": ""
    },
    "Meta": {
      "AppId": "",
      "AppSecret": ""
    }
  },
  "FrontendUrl": "https://dpqbvdgnenckf.cloudfront.net",
  "AllowedOrigins": [
    "https://dpqbvdgnenckf.cloudfront.net",
    "http://localhost:4200"
  ],
  "EventBridge": {
    "EventBusName": "amesa-event-bus",
    "Source": "amesa.auth-service"
  },
  "ConnectionStrings": {
    "Redis": ""
  },
  "CacheConfig": {
    "RedisConnection": "",
    "InstanceName": "amesa-auth",
    "UseSSL": false
  },
  "XRay": {
    "ServiceName": "amesa-auth-service",
    "Enabled": true
  },
  "AdminSettings": {
    "Email": "admin@amesa.com",
    "Password": "Admin123!"
  },
  "Aws": {
    "Region": "eu-north-1"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/auth-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}

